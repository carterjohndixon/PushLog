  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "npx vite build && npx esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --sourcemap --define:__APP_ENV__='\"production\"'",
    "build:carter": "cd carter.pushlog.ai && npm install && npm run build",
    "build:production": "NODE_ENV=production npm run build && npm run build:carter",
    "start": "NODE_ENV=production node dist/index.js",
    "start:dev": "NODE_ENV=development tsx server/index.ts",
    "check": "tsc",
    "check:bundle-size": "npm run build && ls -lh dist/public/js/*.js | awk '{print $5, $9}' && echo '\nVendor chunk should be under 800KB'",
    "db:backup": "node scripts/backup-db.js",
    "db:push": "drizzle-kit push",
    "health": "curl -f http://localhost:5001/health || exit 1",
    "deploy:check": "npm run check && npm run build && npm run health",
    "test:performance": "node performance-test.js",
    "test:database": "node database-performance.js",
    "test:get-stats": "node test/test-get-stats-performance.js",
    "test:push-events": "node test/test-push-events-performance.js",
    "test:analytics": "node test/test-analytics-performance.js",
    "test:notifications": "node test/test-notifications-performance.js",
    "test:ai-usage": "node test/test-ai-usage-performance.js",
    "test:search": "tsx test/test-search.ts",
    "seed:notifications": "node test/seed-notifications-for-user.js",
    "monitor:performance": "node monitor-performance.js",
    "db:add-indexes": "node add-indexes.js",
    "test:ai-performance": "node ai-performance-test.js",
    "test:load": "node load-test.js",
    "test:all-performance": "npm run test:performance && npm run test:database && npm run test:ai-performance && npm run test:load",
    "setup:database-backup": "node setup-database-backup.js",
    "fix:supabase-security": "node fix-supabase-security.js",
    "test:security": "node test-security.js",
    "get:supabase-credentials": "node get-supabase-credentials.js",
    "enable:leaked-password-protection": "node enable-leaked-password-protection.js",
    "check:supabase-security": "node check-supabase-security-status.js"
